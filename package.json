{
  "name": "html-class-suggestions",
  "displayName": "HTML Class Suggestions",
  "description": "Adds code completion suggestions to HTML class attributes based on the CSS files in your workspace.",
  "version": "0.1.0",
  "publisher": "AndersEAndersen",
  "engines": {
    "vscode": "^1.0.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onLanguage:html"
  ],
  "main": "./out/src/extension",
  "scripts": {
    "vscode:prepublish": "node ./node_modules/vscode/bin/compile",
    "compile": "node ./node_modules/vscode/bin/compile -watch -p ./",
    "postinstall": "node ./node_modules/vscode/bin/install",
    "clean": "rimraf out/src/*"
  },
  "devDependencies": {
    "typescript": "^1.8.5",
    "vscode": "^0.11.0",
    "rimraf": "latest"
  },
  "dependencies": {
    "css": "^2.2.1"
  },
  "license": "MIT"
}
