{
  "name": "html-class-suggestions",
  "displayName": "HTML Class Suggestions",
  "description": "Adds code completion suggestions to HTML class attributes based on the CSS files in your workspace.",
  "version": "1.0.7",
  "categories": [
    "Languages",
    "Other"
  ],
  "keywords": [
    "html",
    "css"
  ],
  "publisher": "AndersEAndersen",
  "author": {
    "name": "Anders Ellensh√∏j Andersen",
    "email": "andersa@atlab.dk"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/andersea/HTMLClassSuggestionsVSCode"
  },
  "bugs": {
    "url": "https://github.com/andersea/HTMLClassSuggestionsVSCode/issues"
  },
  "icon": "images/icon.png",
  "license": "MIT",
  "preview": false,
  "engines": {
    "vscode": "^1.0.0"
  },
  "activationEvents": [
    "onLanguage:html",
    "onLanguage:php",
    "onLanguage:vue"
  ],
  "main": "./out/src/extension",
  "scripts": {
    "vscode:prepublish": "tsc -p ./",
    "compile": "tsc -watch -p ./",
    "postinstall": "node ./node_modules/vscode/bin/install",
    "clean": "rimraf out/src/*"
  },
  "devDependencies": {
    "@types/css": "0.0.30",
    "@types/lodash.flow": "^3.5.2",
    "@types/mocha": "^2.2.40",
    "@types/node": "^6.0.40",
    "mocha": "^3.2.0",
    "rimraf": "^2.6.1",
    "tslint": "^5.1.0",
    "typescript": "^2.2.2",
    "vscode": "^1.1.0"
  },
  "dependencies": {
    "css": "^2.2.1",
    "lodash.flow": "^3.5.0",
    "xxhashjs": "^0.2.1"
  }
}
