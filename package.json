{
  "name": "html-class-suggestions",
  "displayName": "HTML Class Suggestions",
  "description": "Adds code completion suggestions to HTML class attributes based on the CSS files in your workspace.",
  "version": "1.1.1",
  "categories": [
    "Programming Languages",
    "Other"
  ],
  "keywords": [
    "html",
    "css"
  ],
  "publisher": "AndersEAndersen",
  "author": {
    "name": "Anders Ellensh√∏j Andersen",
    "email": "andersa@atlab.dk"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/andersea/HTMLClassSuggestionsVSCode"
  },
  "bugs": {
    "url": "https://github.com/andersea/HTMLClassSuggestionsVSCode/issues"
  },
  "icon": "images/icon.png",
  "license": "MIT",
  "preview": false,
  "engines": {
    "vscode": "^1.54.0"
  },
  "activationEvents": [
    "onLanguage:html",
    "onLanguage:cshtml",
    "onLanguage:php",
    "onLanguage:vue",
    "onLanguage:typescriptreact",
    "onLanguage:javascript"
  ],
  "main": "./out/src/extension",
  "scripts": {
    "vscode:prepublish": "tsc -p ./",
    "compile": "tsc -watch -p ./",
    "clean": "rimraf out/src/*",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/css": "0.0.31",
    "@types/glob": "^7.1.3",
    "@types/lodash.flow": "^3.5.6",
    "@types/mocha": "^8.2.1",
    "@types/node": "^14.14.35",
    "@types/vscode": "^1.54.0",
    "glob": "^7.1.6",
    "mocha": "^10.2.0",
    "rimraf": "^3.0.2",
    "tslint": "^6.1.3",
    "typescript": "^4.2.3",
    "vscode-test": "^1.5.1"
  },
  "dependencies": {
    "css": "^3.0.0",
    "lodash.flow": "^3.5.0",
    "xxhashjs": "^0.2.2"
  }
}
